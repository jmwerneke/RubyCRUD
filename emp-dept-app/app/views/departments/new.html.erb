<h3>Create a Department</h3>
<%= form_with model: @department do |form| %>
  <%= form.text_field :dept_num, placeholder: "department number" %>
  <%= form.text_field :dept_name, placeholder: "department name" %>

  <%= form.submit %>
<% end %>

